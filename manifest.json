{
    "manifest_version": 2,
    "name": "JumpingMailSorter",
    "description": "Sorts E-Mails from the all over inbox folder into different local folders as configured",
    "version": "1.0",
    "author": "Robert Duck",
    "browser_specific_settings": {
        "gecko": {
            "id": "JumpingMailSorter@jumpingkey.de",
            "strict_min_version": "78.0"
        }
    },
    "browser_action": {
        "default_popup": "mailSorter/mailSorter.html",
        "default_title": "Mails to sort",
        "default_icon": "images/mailsorter_icon.png"
    },
    "icons": {
        "64": "images/mailsorter_icon.png",
        "32": "images/mailsorter_icon.png",
        "16": "images/mailsorter_icon.png"
    },
    "permissions": [
        "accountsRead"
    ]
}